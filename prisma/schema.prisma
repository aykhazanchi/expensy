generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())

  username String

  expenses Expense[]
}

model Expense {
  id Int @id @default(autoincrement())

  amount Decimal

  createdAt DateTime @default(now())

  merchant String

  userId Int

  user User @relation(fields: [userId], references: [id])
}